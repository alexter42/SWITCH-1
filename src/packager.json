{
  "asarUnpack": [
    "**/programs/server/assets/app/*",
    "resources/mongod",
    "resources/mongod.exe"
  ],
  "appId": "duat.template.desktopapp",
  "electronVersion": "1.7.4",
  "npmRebuild": true,
  "asar": true,
  "files": [
    "**/*",
    "**/.*"
  ],
  "mac": {
    "category": "public.app-category.productivity",
    "icon": ".assets/osx/desktopapp.icns"
  },
  "dmg": {
    "title": "Desktopapp",
    "icon": ".assets/osx/desktopapp.icns",
    "background": ".assets/osx/installer.png",
    "iconSize": 80,
    "contents": [
      {
        "x": 438,
        "y": 344,
        "type": "link",
        "path": "/Applications"
      },
      {
        "x": 192,
        "y": 344,
        "type": "file"
      }
    ]
  },
  "win": {
    "icon": ".assets/win/desktopapp.ico"
  },
  "linux": {
    "icon": ".assets/linux",
    "target": [
      "deb",
      "AppImage"
    ]
  },
  "directories": {
    "app": ".app",
    "output": ".dist"
  }
