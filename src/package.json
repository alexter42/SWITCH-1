{
  "name": "SWITCH",
  "productName": "SWITCH-Desktop",
  "description": "Desktop application for the Renewable and Appropriate Energy Lab's SWITCH python code.",
  "version": "0.0.2",
  "company": "",
  "homepage": "",
  "author": {
    "name": "Alejandra Monroy",
    "email": "lexie@protonmail.com",
    "url": ""
  },
  "private": true,
  "scripts": {
    "config": "meteor yarn add bcrypt --ignore-engines",
    "dev": "node .scripts/run",
    "reset": "meteor reset",
    "build:osx": "node .scripts/download --platform=darwin --arch=x64 && node .scripts/prepare --platform=darwin --arch=x64 && build --mac --config ./packager.json",
    "build:linux:x86": "node .scripts/download --platform=linux --arch=x86 && node .scripts/prepare --platform=linux --arch=x86 && build --linux --ia32 --config ./packager.json",
    "build:linux:x64": "node .scripts/download --platform=linux --arch=x64 && node .scripts/prepare --platform=linux --arch=x64 && build --linux --x64 --config ./packager.json",
    "build:win:x86": "node .scripts/download --platform=win32 --arch=x86 && node .scripts/prepare --platform=win32 --arch=x86 && build --win --ia32 --config ./packager.json",
    "build:win:x64": "node .scripts/download --platform=win32 --arch=x64 && node .scripts/prepare --platform=win32 --arch=x64 && build --win --x64 --config ./packager.json",
    "lint": "eslint ./lib ./client ./server --ext .js --ext .jsx",
    "lintfix": "npm run lint -- --fix",
    "testonly": "mocha meteor/client/**/tests/**/*.js --compilers js:babel-core/register",
    "test": "npm run lint && npm run testonly",
    "test-watch": "npm run testonly -- --watch --watch-extensions js,jsx"
  },
  "dependencies": {
    "apollo-client": "^1.4.2",
    "apollo-upload-client": "^5.1.0",
