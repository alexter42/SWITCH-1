    "graphql-tools": "^1.0.0",
    "leaflet": "^1.0.3",
    "lodash": "^4.17.4",
    "meteor-apollo-accounts": "^2.0.1",
    "meteor-node-stubs": "~0.2.4",
    "react": "^15.6.1",
    "react-apollo": "^1.4.2",
    "react-burger-menu": "^2.1.1",
    "react-dom": "^15.6.1",
    "react-dropzone-component": "^2.0.0",
